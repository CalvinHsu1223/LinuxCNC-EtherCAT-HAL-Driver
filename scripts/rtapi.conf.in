# DO NOT EDIT THIS FILE !
#
# @configure_input@ 
# on @DATE@
#

# A few parameters from emc2/Makefile.inc
EMC2_HOME=@EMC2_HOME@
KERNELDIR=@KERNELDIR@
RTLIB_DIR=@EMC2_RTLIB_DIR@
MODULE_EXT=@MODEXT@
RTPREFIX=@RTPREFIX@
KERNEL_VERS=@KERNEL_VERS@

# Sets the default DEBUG level
DEBUG='1'

# Path to the realtime kernel modules
MODPATH=@MODULE_DIR@

# List of realtime kernel modules to be loaded
if [ "@RTAI@" = "3" ] ; then \
    MODULES="adeos rtai_hal rtai_ksched rtai_fifos rtai_shm rtai_sem rtai_math"
    RTAI=@RTAI@
    MODPATH_adeos=@MODPATH_adeos@
    MODPATH_rtai_hal=@MODPATH_rtai_hal@
    MODPATH_rtai_ksched=@MODPATH_rtai_ksched@
    MODPATH_rtai_fifos=@MODPATH_rtai_fifos@
    MODPATH_rtai_shm=@MODPATH_rtai_shm@
    MODPATH_rtai_sem=@MODPATH_rtai_sem@
    MODPATH_rtai_math=@MODPATH_rtai_math@
elif [ $RTPREFIX = rtai ] ; then
    MODULES="adeos rtai rt_mem_mgr rtai_sched rtai_fifos rtai_shm rtai_libm"
    MODPATH_adeos=@MODPATH_adeos@
    MODPATH_rtai=@MODPATH_rtai@
    MODPATH_rt_mem_mgr=@MODPATH_rt_mem_mgr@
    MODPATH_rtai_sched=@MODPATH_rtai_sched@
    MODPATH_rtai_shm=@MODPATH_rtai_shm@
    MODPATH_rtai_libm=@MODPATH_rtai_libm@
elif [ $RTPREFIX = rtl ] ; then
    MODULES="rtl mbuff rtl_time rtl_sched rtl_posixio rtl_fifo"
    MODPATH_rtl=@MODPATH_rtl@
    MODPATH_mbuff=@MODPATH_mbuff@
    MODPATH_rtl_time=@MODPATH_rtl_time@
    MODPATH_rtl_sched=@MODPATH_rtl_sched@
    MODPATH_rtl_posixio=@MODPATH_rtl_posixio@
    MODPATH_rtl_fifo=@MODPATH_rtl_fifo@
fi
