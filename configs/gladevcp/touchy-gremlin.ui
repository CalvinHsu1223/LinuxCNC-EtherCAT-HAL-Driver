            <property name="double_buffered">False</property>
            <property name="view">y</property>
            <property name="enable_dro">False</property>
          </object>
          <packing>
            <property name="left_attach">1</property>
            <property name="right_attach">2</property>
          </packing>
        </child>
        <child>
          <object class="HAL_Gremlin" id="hal_gremlin3">
            <property name="visible">True</property>
            <property name="double_buffered">False</property>
            <property name="view">x</property>
            <property name="enable_dro">False</property>
          </object>
          <packing>
            <property name="top_attach">1</property>
            <property name="bottom_attach">2</property>
          </packing>
        </child>
        <child>
          <object class="HAL_Gremlin" id="hal_gremlin4">
            <property name="visible">True</property>
            <property name="double_buffered">False</property>
            <property name="view">p</property>
            <property name="enable_dro">False</property>
          </object>
          <packing>
            <property name="left_attach">1</property>
            <property name="right_attach">2</property>
            <property name="top_attach">1</property>
            <property name="bottom_attach">2</property>
          </packing>
        </child>
      </object>
    </child>
  </object>
</interface>
